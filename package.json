    "@sveltejs/adapter-netlify": "5.0.2",
    "@sveltejs/kit": "2.21.4",
    "@sveltejs/vite-plugin-svelte": "5.1.0",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.10",
    "bits-ui": "2.6.0",
    "clsx": "2.1.1",
    "embla-carousel-svelte": "8.6.0",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
